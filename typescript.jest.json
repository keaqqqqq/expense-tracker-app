{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "module": "commonjs",
      "types": ["jest", "@testing-library/jest-dom", "node"],
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"] 
      }
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "jest.setup.ts",
      "jest.config.ts"
    ]
  }